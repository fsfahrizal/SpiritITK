    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:background="#2C3E50"
    android:layout_height="match_parent"
    app:layout_behavior="@string/appbar_scrolling_view_behavior"
    tools:context="com.example.fahrizal.spirititk.TeknikElektro"
    tools:showIn="@layout/activity_teknik_elektro">

    <LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        xmlns:tools="http://schemas.android.com/tools"
        android:id="@+id/content_teknik_elektro"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical"
        android:paddingBottom="@dimen/activity_vertical_margin"
        android:paddingLeft="@dimen/activity_horizontal_margin"
        android:paddingRight="@dimen/activity_horizontal_margin"
        app:layout_behavior="@string/appbar_scrolling_view_behavior"
        tools:context="com.example.fahrizal.spirititk.TeknikElektro">

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="16dp"
            android:text="TEKNIK ELEKTRO"
            android:textColor="#ffffff"
            android:textSize="16sp"
            android:textStyle="bold" />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="10dp"
            android:text="Program studi Teknik Elektro ITK didirikan sebagai pusat penyelenggaraan pendidikan dibidang kelistrikan dengan melakukan kajian terhadap kebutuhan dunia industri saat ini sebagai bahan referensi sehingga pembelajaran yang diberikan dapat disesuaikan dengan kebutuhan industri saat ini dan kedepannya."
            android:textColor="#ffffff"
            android:textSize="14sp" />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="10dp"
            android:text="Sebagai pusat penyelenggaraan pendidikan dibidang kilistrikan, program studi Teknik Elektro ITK juga melakukan penelitian-penelitian yang berkaitan dengan dunia elektro dalam rangka peningkatan sumber daya manusia baik mahasiswa teknik elektro maupun masyarakat. Program studi Teknik Elektro memberikan kesempatan untuk mengembangkan kemampuan dengan memfokuskan pada bidang konversi energi, bidang sistem tenaga, teknologi informasi dan komunikasi dan bidang otomasi industri."
            android:textColor="#ffffff"
            android:textSize="14sp" />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="10dp"
            android:text="Perkembangan teknologi yang ada saat ini mendorong program studi Teknik Elektro untuk menghasilkan lulusan-lulusan yang kompeten di bidang teknik elektro."
            android:textColor="#ffffff"
            android:textSize="14sp" />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="10dp"
            android:text="Untuk mewujudkan tujuan tersebut, proses pembelajaran yang diterapkan selain pembekalan teori-teori dasar juga pembelajaran melalui simulasi sistem tenaga listrik dan implementasi tentang hal-hal yang telah dipelajari."
            android:textColor="#ffffff"
            android:textSize="14sp" />

        <View
            android:layout_width="fill_parent"
            android:layout_height="1dp"
            android:background="#c0c0c0"
            android:layout_marginTop="5dp"
            />

        <TextView
            android:layout_width="wrap_content"
